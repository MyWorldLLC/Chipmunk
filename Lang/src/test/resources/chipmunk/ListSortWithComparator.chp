module test

class Comparator {

    def compare(a, b){
        if(a > b){
            return 1
        }else if(a < b){
            return -1
        }else{
            return 0
        }
    }

}

def main(){
	var list = [3, 1, 2]
	return list.sort(Comparator.new())
}